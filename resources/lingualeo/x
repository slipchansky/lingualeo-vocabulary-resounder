<script>
    var _tracker = _tracker || [];

    function getPusher(eventType, customArray) {
        return function () {
            if ($(this).is(':visible')) {
                var argv = this.getAttribute('data-oid-track').split(',');
                (customArray || _tracker).push([eventType, argv[0], argv[1]]);
            }
        };
    }

    function trackAllOids(eventType, rootElement) {
        var events = [];
        $(rootElement || document.body).find('[data-oid-track]').each(getPusher(eventType, events));
        _tracker.push.apply(_tracker, events);
    };

    $(document.body).on('mousedown', '[data-oid-track]', getPusher('click'));
    trackAllOids('view', document.body);
</script>
